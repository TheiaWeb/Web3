{
  "firestore": {
    "rules": "firestore.rules",
    "indexes": "firestore.indexes.json"
  },
  "functions": [
    {
      "source": "functions",
      "codebase": "default",
      "ignore": [
        "node_modules",
        ".git",
        "firebase-debug.log",
        "firebase-debug.*.log"
      ]
    }
  ],
    "hosting": {
      "public": "public",
      "rewrites": [
        {
          "source": "/",
          "destination": "https://3spacex.com/",
          "type": 301
        }
      ]
    }
}
